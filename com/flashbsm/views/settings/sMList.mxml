<?xml version="1.0" encoding="utf-8"?>
<mx:HBox
	xmlns:mx="http://www.adobe.com/2006/mxml"
	width="100%" height="100%">
	<mx:Script>
		<![CDATA[

			import mx.collections.ArrayCollection;

			[Bindable]
			public var lists:ArrayCollection;
		]]>
	</mx:Script>

	<mx:Repeater id="RLists" dataProvider="{lists}">
		<mx:Label text="{'____' + RLists.currentItem.ShortDesc}"/>
	</mx:Repeater>
</mx:HBox>
